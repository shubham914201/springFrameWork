package com.mavin.shubham;

import org.springframework.stereotype.Component;

@Component
public class Car implements Vehicle {
	@
	private Tyre_2 tyre;
	public Tyre_2 getTyre() {
		return tyre;
	}
	public void setTyre(Tyre_2 tyre) {
		this.tyre = tyre;
	}
	public void drive() {
  	  System.out.println("car"+tyre);
    }
}
